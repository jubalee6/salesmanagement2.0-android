<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/selector_rectangle_8dp_white_none"
    android:padding="@dimen/dp16"
    >
    <!--업체명-->
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_company_name"
        style="@style/StyleTextView.Bold.Medium.Normal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <!--프로그램명-->
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_program_name"
        style="@style/StyleTextView.Bold.Medium.Primary"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/textview_company_name"
        />
    <!--업체 고유번호-->
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_comcode"
        style="@style/StyleTextView.Bold.Medium.Normal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_company_name"
        />
    <!--대표자명-->
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_boss"
        style="@style/StyleTextView.Bold.Small.Normal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_program_name"
        />
    <!--사업번호-->
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_company_number"
        style="@style/StyleTextView.Regular.Small.Normal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_comcode"
        />
    <!--주소-->
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_address"
        style="@style/StyleTextView.Regular.Small.Normal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_company_number"
        />

</androidx.constraintlayout.widget.ConstraintLayout>